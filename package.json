{
  "license": "MIT",
  "name": "contract-viewer",
  "displayName": "Contract Viewer",
  "description": "Contract Viewer",
  "version": "0.0.4",
  "publisher": "Metaplasia",
  "repository": {
    "type": "git",
    "url": "https://github.com/MetaplasiaTeam/vscode-contract-viewer"
  },
  "author": {
    "name": "Metaplasia",
    "email": "metaplasiaio@gmail.com",
    "url": "https://metaplasia.io"
  },
  "homepage": "https://github.com/MetaplasiaTeam/vscode-contract-viewer",
  "bugs": {
    "url": "https://github.com/MetaplasiaTeam/vscode-contract-viewer/issues",
    "email": "metaplasiaio@gmail.com"
  },
  "icon": "image/logo.png",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "solidity",
    "contract",
    "viewer",
    "ethereum",
    "blockchain",
    "eth"
  ],
  "activationEvents": [
    "*",
    "onUri"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "contract-viewer.getContract",
        "category": "Contract Viewer",
        "title": "%command.getContract.title%"
      },
      {
        "command": "contract-viewer.configApi",
        "category": "Contract Viewer",
        "title": "%command.configApi.title%"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%command.configApi.title%",
      "properties": {
        "contract-viewer.setting.api.eth": {
          "type": "string",
          "description": "Ethereum API",
          "default": ""
        },
        "contract-viewer.setting.api.bsc": {
          "type": "string",
          "description": "Binance API",
          "default": ""
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^12.20.46",
    "@types/vscode": "^1.37.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5",
    "vscode-nls-dev": "^4.0.0",
    "vscode-nls-i18n": "^0.2.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@types/shelljs": "^0.8.11",
    "axios": "^0.25.0",
    "fs-extra": "^8.0.1",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.5",
    "typedi": "^0.10.0",
    "vscode-nls": "^5.0.0"
  }
}
